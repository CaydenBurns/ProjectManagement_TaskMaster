<Application x:Class="TM.APP.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TM.APP.UI"
             xmlns:vm="clr-namespace:TM.APP.UI.ViewModels"
             xmlns:global="clr-namespace:"
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:views="clr-namespace:TM.APP.UI.Views"

             >
    <Application.Resources>
        <DataTemplate DataType="{x:Type TypeName=vm:HomeViewModel}">
            <views:HomePage></views:HomePage>
        </DataTemplate>

        <DataTemplate DataType="{x:Type TypeName=vm:UserViewModel}">
            <views:UserView></views:UserView>
        </DataTemplate>

    </Application.Resources>
</Application>