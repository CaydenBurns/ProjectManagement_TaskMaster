<UserControl x:Class="TM.APP.UI.Views.HomePage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TM.APP.UI.Views"
             xmlns:viewModels="clr-namespace:TM.APP.UI.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             DataContext="viewModels:HomeViewModel">
    <Grid Background="Beige">
        <StackPanel>


            <TextBlock FontSize="70"
                       VerticalAlignment="Center"
                       Foreground="Black"
                       HorizontalAlignment="Center">Welcome To HomeView</TextBlock>

            <Button Content="Home"
                    Width="100"
                    Background="SaddleBrown"
                    Foreground="AliceBlue"
            ></Button>
        </StackPanel>


    </Grid>
</UserControl>
